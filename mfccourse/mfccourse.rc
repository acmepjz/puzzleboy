        BEGIN
            MENUITEM "选择(&S)",                      ID_EDIT_SELECT
            MENUITEM SEPARATOR
            MENUITEM "地面(&G)",                      ID_EDIT_BRICK0
            MENUITEM "墙壁(&W)",                      ID_EDIT_BRICK1
            MENUITEM "深坑(&I)",                      ID_EDIT_BRICK2
            MENUITEM "箱子目标(&T)",                    ID_EDIT_BRICK3
            MENUITEM "终点(&E)",                      ID_EDIT_BRICK4
            MENUITEM "玩家(&P)",                      ID_EDIT_BRICK5
            MENUITEM SEPARATOR
            MENUITEM "添加箱子(&B)",                    ID_EDIT_PUSHABLE0
            MENUITEM "添加旋转砖块(&R)",                  ID_EDIT_PUSHABLE1
            MENUITEM "添加目标箱子(&A)",                  ID_EDIT_PUSHABLE2
        END
        POPUP "选择的砖块(&B)"
        BEGIN
            MENUITEM "向上移动(&U)\tUp",                ID_EDIT_MOVE_UP
            MENUITEM "向下移动(&D)\tDown",              ID_EDIT_MOVE_DOWN
            MENUITEM "向左移动(&L)\tLeft",              ID_EDIT_MOVE_LEFT
            MENUITEM "向右移动(&R)\tRight",             ID_EDIT_MOVE_RIGHT
            MENUITEM SEPARATOR
            MENUITEM "顺时针旋转(&W)\tPgDn",             ID_EDIT_ROTATE_CW
            MENUITEM "逆时针旋转(&C)\tPgUp",             ID_EDIT_ROTATE_CCW
            MENUITEM SEPARATOR
            MENUITEM "应用修改(&A)\tEnter",             ID_EDIT_APPLY_CHANGES
            MENUITEM "放弃修改(&I)\tEsc",               ID_EDIT_DISCARD_CHANGES
        END
        MENUITEM "关卡属性(&I)...\tCtrl+Enter",     ID_EDIT_PROPERTIES
        MENUITEM SEPARATOR
        MENUITEM "添加关卡(&D)...",                 ID_EDIT_ADD_LEVEL
        MENUITEM "删除关卡(&V)",                    ID_EDIT_REMOVE_LEVEL
        MENUITEM SEPARATOR
        MENUITEM "剪切(&T)\tCtrl+X",              ID_EDIT_CUT
        MENUITEM "复制(&C)\tCtrl+C",              ID_EDIT_COPY
        MENUITEM "粘贴(&P)\tCtrl+V",              ID_EDIT_PASTE
        MENUITEM "删除(&L)\tDel",                 ID_EDIT_CLEAR
        MENUITEM SEPARATOR
        MENUITEM "全选(&A)\tCtrl+A",              ID_EDIT_SELECT_ALL
    END
    POPUP "视图(&V)"
    BEGIN
        POPUP "工具栏(&T)"
        BEGIN
            MENUITEM "标准(&S)",                      ID_VIEW_TOOLBAR
            MENUITEM "编辑(&E)",                      IDR_EDIT_TOOLBAR
        END
        MENUITEM "状态栏(&S)",                     ID_VIEW_STATUS_BAR
        POPUP "Lan&guage"
        BEGIN
            MENUITEM "&English",                      ID_LANGUAGE_1
            MENUITEM "&Chinese",                      ID_LANGUAGE_99
        END
        MENUITEM SEPARATOR
        MENUITEM "显示网格(&G)\tCtrl+'",            ID_VIEW_GRID
        POPUP "动画速度(&E)"
        BEGIN
            MENUITEM "加速(&F)\tCtrl+=",              ID_VIEW_ANIMATION_SPEED_0
            MENUITEM "减速(&S)\tCtrl+-",              ID_VIEW_ANIMATION_SPEED_00
            MENUITEM SEPARATOR
            MENUITEM "&1x",                         ID_VIEW_ANIMATION_SPEED
            MENUITEM "&2x",                         ID_VIEW_ANIMATION_SPEED_2
            MENUITEM "&4x",                         ID_VIEW_ANIMATION_SPEED_3
            MENUITEM "&8x",                         ID_VIEW_ANIMATION_SPEED_999
        END
        MENUITEM SEPARATOR
        MENUITEM "第一关(&F)\tCtrl+Home",          ID_FIRST_LEVEL
        MENUITEM "上一关(&P)\tCtrl+PgUp",          ID_PREV_LEVEL
        MENUITEM "下一关(&N)\tCtrl+PgDn",          ID_NEXT_LEVEL
        MENUITEM "最后一关(&A)\tCtrl+End",          ID_LAST_LEVEL
        MENUITEM SEPARATOR
        MENUITEM "关卡列表(&L)...\tCtrl+L",         ID_LEVEL_LIST
    END
    POPUP "工具(&T)"
    BEGIN
        MENUITEM "关卡录像(&O)...",                 ID_GAME_RECORD
        MENUITEM SEPARATOR
        MENUITEM "关卡数据库(&D)...",                ID_LEVEL_DATABASE
